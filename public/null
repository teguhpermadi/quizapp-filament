$questionsDoesntHaveParagraph = ExamQuestionParagraph::where('exam_id', $examId)->doesnthave('paragraph')->get();


$data = [];

foreach ($questionsHasParagraph as $questionHasParagraph => $valueParagraph) {
  $data[$valueParagraph[0]['order']] = [
	'paragraph_id' => $questionHasParagraph,
	'questions' => $valueParagraph->pluck('question_id'),
  ];
}

foreach ($questionsDoesntHaveParagraph as $questionDoesntHaveParagraph) {
  $data[$questionDoesntHaveParagraph->order] = [
	'paragraph_id' => null,
	'question' => $questionDoesntHaveParagraph,
  ];
}

$format->collect($data);

$format->sort();
